//설탕배달
//2839번
//경우의수를 구해서 전부if문돌림.. ㅠ
#include <iostream>
using namespace std;
int main()
{
	int n,x;
	cin>>n;
	if(n==4||n==7)
	{
		cout<<"-1";
		return 0;
	}
	
	if(n%5==0) 		cout<<n/5;
	else if(n%5==1) cout<<n/5-1+2;
	else if(n%5==2) cout<<n/5-2+4;
	else if(n%5==3) cout<<n/5+1;
	else if(n%5==4) cout<<n/5-1+3;
}
