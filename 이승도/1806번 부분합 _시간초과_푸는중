//부분합
//1806번
//현재푸는중.. 결과는맞으나 시간초과
#include <iostream>
#include <stdio.h>

using namespace std;
 
int a[100000]={0};

int main()
{
	int N,S,temp,sum,max;
	int count=0;
	int min_count=100001;
	scanf("%d %d",&N,&S);
	for(int i=0;i<N;i++)
	{
		scanf("%d",&a[i]);
	} //입력부분 
	
	
	for(int i=0;i<N;i++)
	{
		sum=0;
		for(int j=i;j<N;j++)
		{
			sum+=a[j];
			if(sum>=S)
			{
				count=j+1-i;
				if(min_count>count)
				{
					min_count=count;
				}
				break;
			}
		}
	}
	if(min_count!=100001)
		printf("%d",min_count);
	else
		printf("0");
}
